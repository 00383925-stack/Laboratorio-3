Algoritmo VerificarNumero
	Definir num Como Entero
	Escribir "Introduce un númmero:"; leer num;
	Si num >= 0 Entonces
		si num < 10 Entonces
			Escribir "El numero esta entre 0 y 10:" 
		SiNo
			Escribir "el numero no esta entre 0 y 10";
		FinSi
	SiNo
		Escribir "el numero no esta entre 0 y 10";
	FinSi
FinAlgoritmo
